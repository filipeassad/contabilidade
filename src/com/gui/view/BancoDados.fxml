	<VBox spacing="10" alignment="CENTER" >
		<Label text="Caminho do banco de dados:"/>	
		<TextField fx:id="tfCcaminho" prefWidth="245" />
		<Button text="Alterar" onAction="#alterar"/>
	</VBox>

	
</VBox>

